</head>
<body>
  <h1>Welcome to Nana Kids Palace/h1>
  <img src="https://cdn.freecodecamp.org/curriculum/cat-photo-app/running-cats.jpg">
  <div id="kids arena"></div>

  <script>
    // Fetch cat photos from Java backend
    fetch("http://localhost:8080/api/catphotos")
      .then(response => response.json())
      .then(data => {
        const container = document.getElementById("cat-photos");
        data.forEach(cat => {
          const img = document.createElement("img");
          img.src = cat.url;
          img.alt = cat.description;
          img.width = 200;
          container.appendChild(img);
        });
      })
      .catch(error => console.error("Error fetching cat photos:", error));
  </script>
</body>
</html>
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;
import java.util.ArrayList;
import java.util.List;
import web.url;
import untitled-1html;
// Spring Boot Application
@SpringBootApplication
public class CatPhotoApp {
    public static void main(String[] args) {
        SpringApplication.run(CatPhotoApp.class, args);
    }
}

// Model class for Cat Photo
class CatPhoto {
    private String url;
    private String description;

    public CatPhoto(String url, String description) {
        this.url = url;
        this.description = description;
    }

    public String getUrl() { return url; }
    public String getDescription() { return description; }
}

// REST Controller for Cat Photos
@RestController
class CatPhotoController {
    @GetMapping("/api/catphotos")
    public List<CatPhoto> getCatPhotos() {
        List<CatPhoto> catPhotos = new ArrayList<>();
        catPhotos.add(new CatPhoto("https://placekitten.com/200/300", "Cute Kitten 1"));
        catPhotos.add(new CatPhoto("https://placekitten.com/250/350", "Cute Kitten 2"));
        catPhotos.add(new CatPhoto("https://placekitten.com/300/400", "Cute Kitten 3"));
        return catPhotos;
    <img background.color src=aqua> 
    }
}
